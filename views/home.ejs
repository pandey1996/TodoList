<html>
    <head>
        <title>TODO App</title>
        <style></style>
    </head>
    <body>
        <h1>TODO App</h1>
        <div id="form">
            <form action="/add" method="GET">
                <input name="desc" id="desc">
                <input name="cat" id="cat">
                <input name="dat" id="dat" type="date">
                <button type="submit">Add Task</button>
            </form>
        </div>
        <div id="display">
            <% for(i=0;i<list.length;i++){ %>
                <div class="loop_box">
                    <h4><%=list[i].description%></h4>
                    <h4><%=list[i].category%></h4>
                    <h4><%=list[i].date%></h4>
                </div>
            <%}%>
        </div>

    </body>
</html>